
from pyspark.sql.functions import expr

# Assuming trust_names_df is a DataFrame containing the Trust Names
trust_names = [
    "Barking, Havering And Redbridge University Hospitals NHS Trust",
    "Barts Health NHS Trust",
    "Beckenham Beacon Ucc",
    "Central London Community Healthcare NHS Trust",
    "Chelsea And Westminster Hospital NHS Foundation Trust",
    "Croydon Health Services NHS Trust",
    "Dmc Vicarage Lane",
    "Edmonton GP Walk In Centre"
]

trust_names_df = spark.createDataFrame([(name,) for name in trust_names], ["Trust_Names"])

# Join the existing DataFrame with the Trust Names DataFrame based on a common key
new_df = new_df.crossJoin(trust_names_df)

# Display the DataFrame
new_df.show(truncate=False)
